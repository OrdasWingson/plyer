<Window x:Class="MassEffectPlyer.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MassEffectPlyer"
        mc:Ignorable="d"
        Title="SettingWindow" Height="455" Width="430" WindowStyle="None" AllowsTransparency="true" MouseLeftButtonDown="Drag_SettingWindow">
    <Control.Background>
        <ImageBrush ImageSource="Image/dialogw.png"/>
    </Control.Background>
    <Grid x:Name="settingWindow" Margin="0,0,-8,-31" Loaded="settingWindow_Loaded">
        <Button x:Name="okButn" Content="Г О Т О В О" HorizontalAlignment="Left" Height="35" Margin="87,414,0,0" VerticalAlignment="Top" Width="292" Style="{DynamicResource DefaultButtonStyle}" BorderThickness="0" FontSize="14" Click="okButn_Click">
            <Control.Background>
                <ImageBrush ImageSource="image/viewbtn.png"></ImageBrush>
            </Control.Background>
        </Button>
        <Slider x:Name="slider" HorizontalAlignment="Left" Height="21" Margin="231,76,0,0" VerticalAlignment="Top" Width="165"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Height="21" Margin="73,76,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="143"/>
    </Grid>
</Window>
